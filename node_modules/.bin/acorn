#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules/acorn/bin/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules/acorn/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/node_modules:/home/fadasgo/Desktop/svelte/node_modules:/home/fadasgo/Desktop/node_modules:/home/fadasgo/node_modules:/home/node_modules:/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules/acorn/bin/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules/acorn/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/node_modules:/home/fadasgo/Desktop/svelte/node_modules:/home/fadasgo/Desktop/node_modules:/home/fadasgo/node_modules:/home/node_modules:/node_modules:/home/fadasgo/Desktop/svelte/svelte-cv/fabiosuarez-fadasgo.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
